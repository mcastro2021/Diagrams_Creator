# Configuración de la aplicación Diagrams Creator
# Copia este archivo como .env y configura tus valores

# Flask Configuration
FLASK_ENV=development
SECRET_KEY=your-secret-key-here-change-in-production

# OpenAI Configuration
OPENAI_API_KEY=
OPENAI_MODEL=gpt-3.5-turbo

# Application Configuration
PORT=5000
LOG_LEVEL=INFO

# AI Configuration
AI_TEMPERATURE=0.7
MAX_TOKENS=2000

# File Upload Configuration
MAX_CONTENT_LENGTH=16777216  # 16MB in bytes

# Database Configuration (if needed in future)
# DATABASE_URL=sqlite:///diagrams.db

# External Services (if needed in future)
# DRAWIO_API_URL=https://app.diagrams.net
# EXPORT_SERVICE_URL=http://localhost:8080
AI_PROVIDER=ollama
OLLAMA_API_KEY=
GROQ_API_KEY=gsk_TU_API_KEY_AQUI
AI_PROVIDER=groq
GROQ_API_KEY=
